script#page-gallery(type="text/x-template")
    div.gallery
        div.gallery__list
            div(v-for="(item, index) in listAr" :style="{height: galleryListHeight + 'px'}").gallery__list-item
                a(:href="item.href" :style="{'background-image': 'url(' + item.imgsrc + ')'}").gallery__list-sitelink
                a(:href="item.gitlink").gallery__list-link.gallery__list-link_git